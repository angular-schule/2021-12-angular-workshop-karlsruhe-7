{
  "name": "book-rating",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "generate:switch-version": "cross-env HTTP_PROXY=http://my-proxy openapi-generator-cli version-manager set 5.3.0",
    "generate:api": "openapi-generator-cli generate -i https://api.angular.schule/swagger.json -g typescript-angular -c config.json -o ./src/app/books/shared/http --skip-validate-spec",
    "generate:api-mit-proxy": "java -Dhttps.proxyHost=internal-proxy.mycompany.com -Dhttps.proxyPort=8080 -jar node_modules/@openapitools/openapi-generator-cli/versions/5.3.0.jar generate -i https://api.angular.schule/swagger.json -g typescript-angular -c config.json -o ./src/app/books/shared/http --skip-validate-spec",
    "generate:api-local-file": "openapi-generator-cli generate -i swagger.json -g typescript-angular -c config.json -o ./src/app/books/shared/http --skip-validate-spec"
  },
  "private": true,
  "dependencies": {
    "@angular-schule/workshop-styles": "^1.3.0",
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "@openapitools/openapi-generator-cli": "^2.4.18",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@ngrx/schematics": "^13.0.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-plugin-ngrx": "^2.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.3"
  }
}
